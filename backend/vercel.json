{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://budget-tracker-app-liard.vercel.app"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}